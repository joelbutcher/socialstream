{
    "name": "joelbutcher/socialstream",
    "description": "Get started with OAuth and Laravel",
    "keywords": [
        "socialstream",
        "laravel",
        "laravel socialite",
        "oauth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joelbutcher/socialstream/issues",
        "source": "https://github.com/joelbutcher/socialstream"
    },
    "authors": [
        {
            "name": "Joel Butcher",
            "email": "joel@joelbutcher.co.uk"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^12.0",
        "laravel/prompts": "^0.3",
        "laravel/socialite": "^5.18"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/sanctum": "^4.0",
        "livewire/livewire": "^3.5.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^10.0",
        "orchestra/testbench-core": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JoelButcher\\Socialstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoelButcher\\Socialstream\\Tests\\": "tests/",
            "App\\": "stubs/app",
            "Database\\Factories\\": "workbench/database/factories/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "7.x-dev"
        },
        "laravel": {
            "providers": [
                "JoelButcher\\Socialstream\\SocialstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
