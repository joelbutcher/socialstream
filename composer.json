{
    "name": "joelbutcher/socialstream",
    "description": "Get started with OAuth and Laravel",
    "keywords": [
        "socialstream",
        "laravel",
        "laravel socialite",
        "oauth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joelbutcher/socialstream/issues",
        "source": "https://github.com/joelbutcher/socialstream"
    },
    "authors": [
        {
            "name": "Joel Butcher",
            "email": "joel@joelbutcher.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.7",
        "illuminate/contracts": "^10",
        "laravel/prompts": "^0.1.10",
        "laravel/socialite": "^5.9",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "laravel/breeze": "^1.26",
        "laravel/jetstream": "^4.0",
        "laravel/sanctum": "^3.3",
        "livewire/livewire": "^3",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "JoelButcher\\Socialstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoelButcher\\Socialstream\\Tests\\": "tests/",
            "App\\": [
                "stubs/app/",
                "workbench/app/"
            ],
            "Database\\Factories\\": "workbench/database/factories/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "JoelButcher\\Socialstream\\SocialstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
